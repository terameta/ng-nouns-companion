{
	"name": "ng-nouns-companion",
	"displayName": "Angular Nouns Companion",
	"description": "Companion Extension for Angular Nouns",
	"repository": "https://github.com/terameta/ng-nouns-companion",
	"publisher": "terameta",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEventsSpecificForStartup": [
		"onStartupFinished"
	],
	"activationEvents": [
		"workspaceContains:ng-nouns.json",
		"onView:ngnouns.nounView"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ng-nouns-companion.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "ng-nouns-companion.startUpHappened",
				"title": "StartUp Happened"
			},
			{
				"command": "ng-nouns.setTNPKey",
				"title": "Angular Nouns - Set The Noun Project Key"
			},
			{
				"command": "ng-nouns.setTNPSecret",
				"title": "Angular Nouns - Set The Noun Project Secret"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "ng-nouns-companion",
					"title": "Angular Nouns Companion",
					"icon": "./dist/assets/ng-nouns-companion.svg"
				}
			]
		},
		"views": {
			"ng-nouns-companion": [
				{
					"id": "ngnouns.nounView",
					"name": "Angular Nouns Companion",
					"type": "webview"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/svgo": "^2.6.3",
		"@types/uuid": "^8.3.4",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"@vscode/test-electron": "^2.1.3",
		"copy-webpack-plugin": "^11.0.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"ts-loader": "^9.2.8",
		"typescript": "^4.6.4",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"axios": "^0.27.2",
		"oauth-1.0a": "^2.2.6",
		"svgo": "^2.8.0",
		"uuid": "^8.3.2"
	}
}